{
  "expo": {
    "name": "formulahacks-2024-frontend",
    "slug": "formulahacks-2024-frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "https",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-router", "expo-font", "expo-location"],
    "fonts": ["node_modules/@expo-google-fonts/inter/Inter_300Light.ttf"],
    "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location.",
    "isAndroidBackgroundLocationEnabled": true,
    "ios": {
      "config": {}
    },
    "extra": {
      "eas": {
        "projectId": "7b11677d-0a8a-4ef8-abc2-61b6062e62bd"
      }
    },
    "android": {
      "package": "example.expo.first"
    }
  }
}
